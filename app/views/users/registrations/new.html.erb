<div class="container">
    <h1 class="well">Registration Form</h1>
	<div class="col-sm-12 well">
    <div class="col-sm-9 col-sm-offset-3">
    <div class="row">
      <div class="col-sm-9">
        <h3 class="text-center">Sign up with Google</h3>
        <br /><%= button_to "Sign up with Google", user_google_oauth2_omniauth_authorize_path, class: 'btn btn-success btn-block' %><br />
        <h3 class="text-center">Or do it the old fashioned way...</h3>
      </div>
    </div>
  </div>
	<div class="row">
				<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
					<div class="col-sm-9 col-sm-offset-3">
						<div class="row">
							<div class="col-sm-9 form-group">
								<%= f.label :username %><br />
								<%= f.text_field :username, :class => "form-control", autofocus: true %>
							</div>
            </div>
            <div class="row">
							<div class="col-sm-9 form-group">
								<%= f.label :email %><br />
								<%= f.email_field :email, :class => "form-control" %>
							</div>
            </div>
            <div class="row">
							<div class="col-sm-9 form-group">
								<%= f.label :password %><br />
								<%= f.password_field :password, :class => "form-control", autocomplete: "off" %>
							</div>
            </div>
            <div class="row">
							<div class="col-sm-9 form-group">
								<%= f.label :password_confirmation %><br />
								<%= f.password_field :password_confirmation, :class => "form-control", autocomplete: "off" %>
							</div>
            </div>
            <div class="row">
              <div class="col-sm-9">
					      <%= f.submit "Sign up", :class => "btn btn-info btn-block"%><br />

              </div>
            </div>

					</div>
				<% end %>
				</div>
	</div>
	</div>
