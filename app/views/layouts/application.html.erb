<!DOCTYPE html>
<html>
<head>
  <title>TaproomIndexRails</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <!-- Google font -->
  <link href='http://fonts.googleapis.com/css?family=Monoton|Open+Sans' rel='stylesheet' type='text/css'>
  <%= yield :index_head %>
</head>
<body>

      <%= render 'layouts/navigation' %>

    <main role="main">
       <%= render 'layouts/messages' %>
       <%= yield %>
    </main>
  </body>
  <footer>
    <div class="footer footer-container">
    </div>
  </footer>
</html>
